{
  "name": "weather-app",
  "version": "1.0.0",
  "description": "Detailed Forecasts Application",
  "default": "index.html",
  "scripts": {
    "watch:sass": "sass sass/main.scss  css/style.css -w",
    "devserver": "live-server",
    "compile:sass": "sass sass/main.scss  css/style.comp.css",
    "prefix:css": "postcss  css/style.comp.css --use autoprefixer  -o css/style.css",
    "build:css": "npm-run-all compile:sass prefix:css  ",
    "start:sass": "npm-run-all --parallel devserver watch:sass",
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir dist --public-url /"
  },
  "author": "Mariam Reda",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.16.1",
    "postcss-cli": "^11.0.1",
    "sass": "^1.94.1",
    "url": "^0.11.4"
  },
  "dependencies": {
    "core-js": "^3.47.0",
    "regenerator-runtime": "^0.14.1"
  }
}
